	#include "Limit.h"

	#include <iostream>
	#include <climits>



	using namespace std;

	double Limit::DoubleMin = numeric_limits<double>::min();

	double Limit::DoubleMax = numeric_limits<double>::max();
	float  Limit::FloatMin = numeric_limits<float>::min();
	float  Limit::FloatMax = numeric_limits<float>::max();


