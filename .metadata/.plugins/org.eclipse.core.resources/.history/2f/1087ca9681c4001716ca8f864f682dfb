	#include "./../ConstraintParser/ConstraintParameter.cuh"

	/*
	 *  所有的约束的比较运算符的类型信息
	 *  1 表示> , >= , < , <=四种情况
	 *  0 表示 == 情况
	 * -1 表示 ！= 情况
	 * */

	/*
	 * 判断是否是 == 比较运算符号的处理
	 * */
	__device__ bool equal(CoodinateDouble* a,CoodinateDouble* b,const int cmpType)
	{
		return (cmpType==ConstantValue::Equal);
	}

	/*
	 * 判断是否是 != 比较运算符号的处理
	 * */
	__device__ bool notEqual(CoodinateDouble* a,CoodinateDouble* b,const int cmpType)
	{
		return (cmpType==ConstantValue::NotEqual);
	}

	/*
	 * 判断是否是> 和 >=比较运算符号的处理
	 *  k==0 && y1 <0 && y2<0  无解
	 *  k==0 && y1 >=0 && y2>=0 全区间
	 *  y1>=0 && y2>=0 全区间
	 *  z>=x1 && z<=x2 && y1>=0
	 *  z>=x1 && z<=x2 && y2>=0
	 *  z<x1 && y1 <0 && y2<0
	 *  z>x2 && y1 <0 && y2<0
	 * */
	__device__ bool great(CoodinateDouble* a,CoodinateDouble* b,const int cmpType)
	{

		return true;
	}


