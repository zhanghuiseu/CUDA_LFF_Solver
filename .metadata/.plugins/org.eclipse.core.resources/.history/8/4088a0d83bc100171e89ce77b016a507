	#include <iostream>
	#include "./../solver/runLFFSolver.h"

	using namespace std;

	int main()
	{

		const int size=10;
		int** a=new int*[size];
		for(int i=0;i<size;i++)
			a[i]=new int[i+1];
		for(int i=0;i<size;i++)
		{
			for(int j=0;j<i+1;j++)
				a[i][j]=j+1;
		}

		for(int i=0;i<size;i++)
		{
			for(int j=0;j<i+1;j++)
				cout<<a[i][j]<<"   "<<(&a[i][j])<<"    ";
			cout<<endl;
		}

		cout<<"LFF Solver Begin:  "<<endl;

		//runLFFSolver::run();

		cout<<"LFF Solver Game Over!!!"<<endl;

	}


