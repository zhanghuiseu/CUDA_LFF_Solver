#ifndef FULLCOVEREDINFO_CUH_
#define FULLCOVEREDINFO_CUH_


/*
 *  这个是为了计算上一轮的预测解而专门设计的一个数据结构，
 *  不过，可能有这样的疑问：为什么不直接使用PriorityDouble或者PriorityFloat呢？
 * */
class FullCoveredInfo
{
public:
	bool isCovered;
	int index;
	FullCoveredInfo()
	{
		this->isCovered =false;
		this->index = -1;
	}
};


#endif /* FULLCOVEREDINFO_CUH_ */
