#ifndef PRIORITYDOUBLE_CUH_
#define PRIORITYDOUBLE_CUH_

class PriorityDouble
{
public:
	double priority;
	bool isFullCovered;

	void setPriority(double priority)
	{
		this->priority = priority;
	}

};

