#ifndef PCATG_H_
#define PCATG_H_

class PCATG
{
public:
	PCATG()
	{

	}
	~PCATG()
	{

	}

	int generateTestDataForSolver(int paramIndex, double* seed, int round,
			int nextRoundSeedIndex, int max_num_of_predict_param);

	/*
	 * 生成初始搜索点，一共有两种可能情况：
	 * 1）给定的起始搜索点
	 * 2）未给定，直接随机生成起初点
	 * */
	void initialParameter(double*& initParams);


	void setRandom(int paramIndex, double value, int mode);

};




#endif /* PCATG_H_ */
