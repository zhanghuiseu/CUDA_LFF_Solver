	#include <iostream>
	#include "./../solver/runLFFSolver.h"

	using namespace std;



	int main()
	{
		cout<<"From Int To Bool"<<endl;
		bool res = -1;
		cout<<res<<endl;
		res = 1;
		cout<<res<<endl;
		res = 0;
		cout<<res<<endl;

		cout<<"From Bool To Int"<<endl;
		res = true;
		int a=res;
		cout<<a<<endl;
		res= false;
		a=res;
		cout<<a<<endl;

		cout<<"LFF Solver Begin:  "<<endl;

		//runLFFSolver::run();

		cout<<"LFF Solver Game Over!!!"<<endl;

	}


