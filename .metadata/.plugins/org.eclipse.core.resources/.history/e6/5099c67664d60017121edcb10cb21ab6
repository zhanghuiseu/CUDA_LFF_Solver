#ifndef PREDICTVALUE_CUH_
#define PREDICTVALUE_CUH_

#include "./../solver/type.h"

/*
 * 预测解以及解的有效性
 * */
class PredictValue
{
public:
	FloatType left;
	FloatType value;
	FloatType right;
	bool isValid;

	PredictValue()
	{
		this->isValid = true;
		this->left = 0.0;
		this->value = 0.0;
		this->right = 0.0;
	}

};


#endif /* PREDICTVALUE_CUH_ */
