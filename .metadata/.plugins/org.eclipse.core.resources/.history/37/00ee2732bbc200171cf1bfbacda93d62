#ifndef FULLCOVEREDINFO_CUH_
#define FULLCOVEREDINFO_CUH_

class FullCoveredInfo
{
public:
	bool isCovered;
	int index;
	FullCoveredInfo()
	{
		this->isCovered =false;
		this->index = -1;
	}
};



#endif /* FULLCOVEREDINFO_CUH_ */
