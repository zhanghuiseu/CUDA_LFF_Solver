#ifndef LIMIT_H_
#define LIMIT_H_

	#include "Limit.h"

	#include <iostream>
	#include <climits>

/*
 * 这个类主要是保存各种可能用到的极值
 * */
class Limit
{
public:

	double DoubleMin = numeric_limits<double>::min();
	double DoubleMax = numeric_limits<double>::max();
	float  FloatMin = numeric_limits<float>::min();
	float  FloatMax = numeric_limits<float>::max();
};

#endif /* LIMIT_H_ */
