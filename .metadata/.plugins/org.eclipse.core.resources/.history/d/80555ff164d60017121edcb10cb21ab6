#ifndef PREDICTVALUEWITHONE_CUH_
#define PREDICTVALUEWITHONE_CUH_

#include "./../solver/type.h"

/*
 * 预测解以及解的有效性
 * */
class PredictValueWithOne
{
public:
	FloatType value;
	bool isValid;

	PredictValueWithOne()
	{
		this->isValid = true;
		this->value = 0.0;
	}
};


#endif /* PREDICTVALUEWITHONE_CUH_ */
