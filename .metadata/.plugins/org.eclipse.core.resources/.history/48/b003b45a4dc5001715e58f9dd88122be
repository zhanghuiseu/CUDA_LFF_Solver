/*
 * 这个类主要是保存各种可能用到的极值
 * */
class Limit
{
public:
	static const double DoubleMin ;
	static const double DoubleMax ;
	static const float  FloatMin ;
	static const float  FloatMax ;
};

#endif /* LIMIT_H_ */
