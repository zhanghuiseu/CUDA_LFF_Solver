	__device__ void getRuntimeValue_0_6(FloatType x , FloatType y , FloatType z , FloatType w , FloatType t , FloatType v , Coodinate* res )
	{
		res->y = ( x ) - ( (w+y-z) );
		res->isCovered = (bool)(res->y >0);
		res->isValid = (bool)(isfinite(res->y));
		return ;
	}

	/*
	 * 第7个约束的CUDA计算函数
